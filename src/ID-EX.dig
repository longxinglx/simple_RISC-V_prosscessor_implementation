        <entry>
          <string>Code</string>
          <string>`define MEM_NOP 4&apos;b1111

module ID_EX(clk,rst,pc,r1,r2,rd,imm,memory_op,alu_op,r1_pc_sel, 
	r2_imm_sel,rf_write,rf_in_sel, rs1, rs2, 
	pc_out,r1_out,r2_out,rd_out,imm_out,memory_op_out,alu_op_out,
	r1_pc_sel_out,r2_imm_sel_out,rf_write_out,
	rf_in_sel_out, rs1_out, rs2_out);

	input clk, rst;
	input [31:0] pc, r1, r2;
	input [4:0] rd;
	input [31:0] imm;
	input [3:0] memory_op, alu_op;
	input r1_pc_sel, r2_imm_sel, rf_write;
	input [1:0] rf_in_sel;
	input [4:0] rs1, rs2;

	output [31:0] pc_out, r1_out, r2_out;
	output [4:0] rd_out;
	output [31:0] imm_out;
	output [3:0] memory_op_out, alu_op_out;
	output r1_pc_sel_out, r2_imm_sel_out, rf_write_out;
	output [1:0] rf_in_sel_out;
	output [4:0] rs1_out, rs2_out;
	
	reg [31:0] pc_out, r1_out, r2_out;
	reg [4:0] rd_out;
	reg [31:0] imm_out;
	reg [3:0] memory_op_out = `MEM_NOP, alu_op_out;
	// pc_load_out 初始值设为1，使得流水线刚启动时能顺利运行下去
	reg r1_pc_sel_out, r2_imm_sel_out, rf_write_out;
	reg [1:0] rf_in_sel_out;
	reg [4:0] rs1_out, rs2_out;
	
	always @(posedge clk)
	begin
		if (rst)
		begin
			pc_out = 0;
			r1_out = 0;
			r2_out = 0;
			rd_out = 0;
			imm_out = 0;
			memory_op_out = `MEM_NOP;
			alu_op_out = 0;
			r1_pc_sel_out = 0;
			r2_imm_sel_out = 0;
			rf_write_out = 0;
			rf_in_sel_out = 0;
			rs1_out = 0;
			rs2_out = 0;
		end
		else
		begin
			pc_out = pc;
			r1_out = r1;
			r2_out = r2;
			rd_out = rd;
			imm_out = imm;
			memory_op_out = memory_op;
			alu_op_out = alu_op;
			r1_pc_sel_out = r1_pc_sel;
			r2_imm_sel_out = r2_imm_sel;
			rf_write_out = rf_write;
			rf_in_sel_out = rf_in_sel;
			rs1_out = rs1;
			rs2_out = rs2;
		end
	end
endmodule</string>
        </entry>
